{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/node"
    },
    {
      "src": "pages/api/auth/[...nextauth].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth/[...nextauth]"
    }
  ]
}
